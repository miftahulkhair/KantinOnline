<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mmift.kantinonline">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.LoginForm"></activity>
        <activity
            android:name=".Activity.AdminActivity"
            android:label="@string/title_daftar_pesanan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.DetailMenuActivity"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Activity.OrderActivity"
            android:parentActivityName=".Activity.MainActivity" />
        <activity
            android:name=".Login.LoginAdminActivity"
            android:parentActivityName=".Login.LoginForm" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.DetailPesananActivity"
            android:label="@string/title_detail_pesanan"
            android:parentActivityName=".Activity.AdminActivity"></activity>
    </application>

</manifest>